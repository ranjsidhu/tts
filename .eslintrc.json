{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-unused-vars": [
      "error",
      { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
    ],
    "import/no-unused-modules": [
      "error",
      {
        "unusedExports": true,
        "missingExports": false,
        "ignoreExports": ["**/page.tsx", "**/not-found.tsx"]
      }
    ]
  },
  "plugins": ["import"]
}
